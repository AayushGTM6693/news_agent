generator client {
  provider = "prisma-client-py"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model NewsAnalysis {
  id          Int      @id @default(autoincrement())
  title       String
  description String?
  url         String
  confidence  Float
  preference  String
  createdAt   DateTime @default(now())
  
  @@map("news_analysis")
}